{
  "name": "threejs-boilerplate",
  "author": "Magnus Brzenk",
  "version": "0.0.0",
  "description": "ThreeJs Boilerplate using typescript and webpack",
  "private": true,
  "license": "MIT",
  "cacheDirectories": [
    "node_modules"
  ],
  "engines": {
    "node": ">=8.9.0"
  },
  "dependencies": {
    "three": "^0.137.0",
    "three-orbit-controls": "^82.1.0",
    "three-orbitcontrols": "^2.102.2"
  },
  "devDependencies": {
    "@types/html-webpack-plugin": "^3.2.0",
    "@types/mini-css-extract-plugin": "^0.2.0",
    "@types/node": "10.14.3",
    "@types/optimize-css-assets-webpack-plugin": "^1.3.4",
    "@types/terser-webpack-plugin": "^1.2.1",
    "@types/uglifyjs-webpack-plugin": "^1.1.0",
    "@types/webpack": "^4.4.32",
    "@types/webpack-bundle-analyzer": "^2.9.2",
    "@types/webpack-dev-server": "^3.1.6",
    "@types/webpack-env": "^1.13.6",
    "@types/webpack-merge": "^4.1.3",
    "@types/webpack-node-externals": "^1.6.3",
    "autoprefixer": "9.5.0",
    "base-href-webpack-plugin": "2.0.0",
    "browser-sync": "2.26.5",
    "browser-sync-webpack-plugin": "2.2.2",
    "cache-loader": "2.0.1",
    "copy-webpack-plugin": "5.0.2",
    "css-hot-loader": "^1.4.4",
    "css-loader": "2.1.1",
    "dotenv": "^8.0.0",
    "file-loader": "3.0.1",
    "fork-ts-checker-webpack-plugin": "1.0.0",
    "friendly-errors-webpack-plugin": "1.7.0",
    "gh-pages": "^2.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^4.0.0-beta.5",
    "http-server": "^0.11.1",
    "husky": "^1.3.1",
    "identity-obj-proxy": "3.0.0",
    "is-wsl": "^2.0.0",
    "json-loader": "0.5.7",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "lint-staged": "8.1.5",
    "merge-jsons-webpack-plugin": "1.0.18",
    "mini-css-extract-plugin": "0.5.0",
    "moment-locales-webpack-plugin": "1.0.7",
    "node-sass": "^4.12.0",
    "open-browser-webpack-plugin": "0.0.5",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "3.0.0",
    "postcss-safe-parser": "^4.0.1",
    "prettier": "1.16.4",
    "react-dev-utils": "^9.0.1",
    "sass": "1.17.3",
    "sass-loader": "7.1.0",
    "simple-progress-webpack-plugin": "1.1.2",
    "source-map-loader": "0.2.4",
    "sourcemap-istanbul-instrumenter-loader": "0.2.0",
    "stats.js": "^0.17.0",
    "stripcomment-loader": "0.1.0",
    "style-loader": "0.23.1",
    "terser-webpack-plugin": "^1.3.0",
    "thread-loader": "2.1.2",
    "to-string-loader": "1.1.5",
    "ts-loader": "5.3.3",
    "ts-node": "8.0.3",
    "tsconfig-paths": "^3.8.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslint": "5.14.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-eslint-rules": "5.4.0",
    "tslint-loader": "3.6.0",
    "typescript": "3.3.4000",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "url-loader": "^2.0.1",
    "webpack": "4.28.4",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "3.3.0",
    "webpack-dev-server": "3.2.1",
    "webpack-merge": "4.2.1",
    "webpack-notifier": "^1.7.0",
    "workbox-webpack-plugin": "4.1.1",
    "write-file-webpack-plugin": "4.5.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "echo '>>> Running Husky' && lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "{server/,server/**/,client/,client/**/}*.{md,json,js,jsx,ts,tsx,css,scss,less,yml}": [
        "prettier --write",
        "git add"
      ]
    },
    "ignore": [
      "**/dist/*"
    ]
  },
  "scripts": {
    "dev": "bash _build_js_bundle --dev",
    "build": "bash _build_js_bundle",
    "analyze": "npm run build && node_modules/.bin/webpack-bundle-analyzer compilation-stats.json"
  },
  "resolutions": {
    "@types/webpack": "^4.4.32"
  }
}
